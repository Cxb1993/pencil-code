!                           -*-f90-*-     (for Emacs)
!
!  Initialisation parameters
!
&init_pars
  cvsid='$Id: start.in,v 1.3 2005-08-10 20:17:38 wlyra Exp $',
  ip=14,                        ! debugging parameter
  xyz0  = -20.,-20.,-20.,       ! first corner of box
  xyz1  =  20., 20., 20.,       ! second corner of box
  lperi =  F  , F  , T,         ! periodic direction?
  !lshift_origin =  F,  F,  F,

  bcx = 'a','s','s','a2'
  bcy = 's','a','s','a2'

  !bcz = 's','s','a','s:a2','s:a2'
  !random_gen='nr_f90',
  r_int=2.,                    ! radius of interior spherical boundary
  r_ext=18.,                   ! radius of exterior spherical boundary
  lcylindrical=T
/
&eos_init_pars
  cs0=0.7
  !rho0=1.,
/
&hydro_init_pars
  !Omega = 2.51588    !for jupiter 5.2 AU
  !Omega=0.39         !for outer edge r_ext=18
  inituu='keplerian'
  !urand=0.05,
  !lcentrifugal_force = T
  !Omega_int = 15.
/
&density_init_pars
  initlnrho='mmsn'
  lnrho_const=1.
/
&grav_init_pars
  ipotential='smoothed-newton', g0=890., r0_pot=0.05,n_pot=10
  ![x]=1 UA and [u] = 1km/s lead to g0=GM=890
  !using [x]= planet distance and [t]=1 orbit, g0=(2pi)**2 

/
