!                           -*-f90-*-     (for Emacs)
!
!  Run parameters
!
&run_pars
  cvsid='$Id: run.in,v 1.4 2005-08-11 10:09:48 wlyra Exp $',
  ip=13,
  nt=1000, it1=20, isave=10, itorder=3, ialive=1
  cdt=0.4, cdtv=0.3, dtmin=1e-6,
  dsnap=0.05, dvid=0.05, d2davg=10.
  !ttransient=0.,
  !tavg = 10.,                   ! time constant for time averaging
  !random_gen='nr_f90',
  !slice_position='e'
/
&eos_run_pars
/
&hydro_run_pars
  !wdamp=0.7,
  !Omega=0.39    !sqrt(g0/r**3)  !in the border
  !Omega = 2.51588  !for jupiter 5.2 AU
  lcalc_turbulence_pars=T
/
&density_run_pars
  lupw_lnrho=T! use upwinding for lnrho to avoid wiggles
  lmass_spline=T 
  wdamp=0.7
  lnrho_int=0.3, lnrho_ext=-0.5
/
&grav_run_pars
  ipotential='geo-kws', g0=890., r0_pot=0.05,n_pot=10
/
&viscosity_run_pars
  nu=5.e-2
/
