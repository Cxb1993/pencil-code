$(INITCOND_OBJ):            initcond.f90         $(CDATA_OBJ) $(MPICOMM_OBJ) $(GENERAL_OBJ) $(FOURIER_OBJ) $(SUB_OBJ) $(EOS_OBJ)
$(TIMESTEP_OBJ):            $(TIMESTEP).f90      $(CDATA_OBJ) $(MPICOMM_OBJ) equ$(PC_META_TYPE).o $(FORCING_OBJ) $(BORDER_PROF_OBJ)
