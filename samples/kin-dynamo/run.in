!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
!  run parameters for kinematic dynamo run with Glen Roberts flow
!
&run_pars
  cvsid='$Id: run.in,v 1.8 2008-03-24 17:04:59 brandenb Exp $',
  nt=50, it1=10, cdt=99, cdtv=.4, old_cdtv=F, isave=10, itorder=3,
  dsnap=50, dvid=0.5,
  random_gen='nr_f90'
  lpencil_check=T
  kinflow='roberts'
  lbidiagonal_derij=F  !(uses the slightly more accurate formulation for derij)
/
&magnetic_run_pars
  eta=0.1
/
