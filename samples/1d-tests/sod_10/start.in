!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
! Initialisation parameters for shock tube problem
!
&init_pars
  cvsid='$Id: start.in,v 1.4 2008-01-10 14:58:08 ajohan Exp $'
  xyz0  = -3.7, 0.0, 0.0,     ! first corner of box
  xyz1  =  6.3, 1.0, 1.0,     ! box size
  lperi = F , T , T,          ! periodic direction?
  lwrite_ic = T               ! Write initial condition to disk
/
&eos_init_pars
/
&hydro_init_pars
/
&density_init_pars
  initlnrho='xjump', rho_left=10., rho_right=1., widthlnrho=.03
/
&entropy_init_pars
  initss='xjump', ss_left=-0.615, ss_right=.306, widthss=.03
/
