&run_pars
  cvsid='$Id: run.in,v 1.4 2005-06-26 17:34:19 eos_merger_tony Exp $'
  nt=5000, it1=10, cdt=0.1, cdtv=0.20, isave=50, itorder=3,
  dsnap=20., dvid=1., tmax=80.
