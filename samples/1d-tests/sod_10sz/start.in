!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
! Initialisation parameters for shocktube problem
!
&init_pars
  cvsid='$Id: start.in,v 1.2 2004-04-07 10:36:27 theine Exp $'
  xyz0  = 0.0, 0.0, -3.7,      ! first corner of box
  xyz1  = 1.0, 1.0, 6.3,      ! box size
  lperi = T , T , F,          ! periodic direction?
/
&hydro_init_pars
/
&density_init_pars
  initlnrho='zjump', rho_left=10., rho_right=1., widthlnrho=.03
/
&entropy_init_pars
  initss='zjump', ss_left=-0.615, ss_right=.306, widthss=.03
/
