  ip=14,                        ! debugging parameter
  xyz0  = -2.1,-2.1,-2.0,       ! first corner of box
  xyz1  =  2.1, 2.1, 2.0,       ! second corner of box
  lperi =  F  , F  , T,         ! periodic direction?
  bcx = 'a','s','s','a2'
  bcy = 's','a','s','a2'
