!                           -*-f90-*-     (for Emacs)
!
!  Initialisation parameters
!
&init_pars
  cvsid='$Id: start.in,v 1.14 2006-02-04 03:51:39 wlyra Exp $',
  ip=14,                        ! debugging parameter
  xyz0  = -2.6,-2.6,-0.26,       ! first corner of box
  xyz1  =  2.6, 2.6, 0.26,       ! second corner of box
  lperi =  F  , F  , T,         ! periodic direction?
  !lshift_origin =  F,  F,  F,

  bcx = 's','s','a','s'!,'s','s','a','s'
  !      velgas, densgas, veldust,densdust
  bcy = 's','s','a','s'!,'s','s','a','s'

  r_int=0.4,                   ! radius of interior spherical boundary
  r_ext=2.5,                   ! radius of exterior spherical boundary
  lcylindrical=T
/
&eos_init_pars
  cs0=0.05 
  rho0 = 1.
/
&hydro_init_pars
  inituu='keplerian'
/
&density_init_pars
  initlnrho='zero' 
  lnrho_const=0.  
  ldensity_nolog=T
/
&grav_init_pars
  ipotential='smoothed-newton', 
  g0=1.,r0_pot=0.1,n_pot=2
/
&particles_init_pars
  initxxp='constant', initvvp='constant'
  xp0=-1,0  
  yp0=0,0
  vpx0=0,0
  vpy0=-1,0
/
&planet_init_pars
  llocal_iso=T
  lcs2_global=T
/

