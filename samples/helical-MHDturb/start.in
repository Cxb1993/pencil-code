!                     -*-f90-*-   (for emacs)
! Initialisation parameters for forced helical MHD turbulence
!
&init_pars
  cvsid='$Id: start.in,v 1.17 2002-08-28 12:27:46 brandenb Exp $',
  random_gen='system'
  random_gen='min_std'
/
&hydro_init_pars
/
&density_init_pars
  gamma=1.
/
&magnetic_init_pars
  initaa='Beltrami-y', amplaa=-0.01  !(+ve amplaa means now positive helicity)
  initaa='gaussian-noise', amplaa=1e-4
/
