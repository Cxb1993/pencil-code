gsl_wrapper_r8.o: gsl_wrapper_r8.c
	$(CC) $(CFLAGS) -c gsl_wrapper_r8.c 
## ---------------------------------------------------------------------- #
## Automatically generatable dependencies
## ---------------------------------------------------------------------- #
start.o:     start.f90 $(start_deps)
run.o:       run.f90 $(run_deps)
