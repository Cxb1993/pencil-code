!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
!  sample file for 1-D sound wave
!
&run_pars
  cvsid='$Id: run.in,v 1.2 2004-04-07 10:36:25 theine Exp $'
  nt=2000000, it1=100, cdt=0.05, cdtv=0.4, isave=50, itorder=3,
  dsnap=20., dvid=.02,tmax=1.5,
  bcx = 'a','s','s','s','s'
/
&hydro_run_pars
/
&density_run_pars
  cdiffrho=0.,  !(mass diffusion not currently used)
/
&entropy_run_pars
  lcalc_heatcond_constchi=T,chi=5e-5
/
&viscosity_run_pars
  nu=0.002, nu_shock=3.5  !(propagates too fast, 6 instead of 5.5)
  nu=0.0005, nu_shock=3.0
/
