$(MESSAGES_OBJ):            messages.f90         $(CDATA_OBJ) $(MPICOMM_OBJ)
$(GHOSTFOLD_OBJ):           $(GHOSTFOLD_SRC)     $(CDATA_OBJ) $(MPICOMM_OBJ) $(MESSAGES_OBJ) $(FOURIER_OBJ)
persist$(PC_META_TYPE).o:   persist.f90          $(CDATA_OBJ) $(GENERAL_OBJ) $(INTERSTELLAR_OBJ) $(FORCING_OBJ)
filter$(PC_META_TYPE).o:    filter.f90           $(CDATA_OBJ) $(SUB_OBJ) boundcond$(PC_META_TYPE).o
