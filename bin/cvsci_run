#!/bin/csh
#$Id: cvsci_run,v 1.3 2002-10-02 20:11:15 dobler Exp $
#
#  This script adds a new run to the repository
#  and checks in all *.in files as well as material
#  from the data and src directories.

cvs add $1
cd $1
set CWD=$cwd
cvs add data src *.in k.dat
cd src
cvs add *.local
cd ..
cd data
cvs add n.dat index.pro
cd $CWD
cvs ci -m "added new run"

echo OK
