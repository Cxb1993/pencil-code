!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
! Initialisation parameters for shocktube problem
!
&init_pars
  cvsid='$Id: start.in,v 1.2 2006-05-19 22:23:12 ajohan Exp $'
  xyz0  = -6.28, -6.28, -6.28   ! first corner of box
  xyz1  =  6.28,  6.28,  6.28   ! box size
  lperi =     T,     T,     T   ! periodic direction?
/
&eos_init_pars
  gamma=1.0, rho0=1.0, cs0=1.0
/
&hydro_init_pars
/
&density_init_pars
  initlnrho='jeans-wave-x', kx_lnrho=0.5, ampllnrho=0.001
/
&selfgravity_init_par
  rhs_selfg=1.0
/
