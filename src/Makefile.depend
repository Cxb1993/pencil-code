#
# Explict dependencies for the Fourier routines to allow them to pull in
# different library routines
#
fft.o :      fft.f
fftpack.o :  fftpack.f
#
nofourier.o:       nofourier.f90 fourier.h $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o
#
fourier_fft.o:     fourier_fft.f90 fft.o fourier.h $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o
#
fourier_fftpack.o: fourier_fftpack.f90 fftpack.o fourier.h $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o
#
#
# Explict dependencies for the Shock routines
# different library routines
#
shock.o:       shock.h   shock.f90 shock_profile.inc $(CDATA_OBJ) $(CPARAM_OBJ) $(SUB_OBJ) boundcond.o $(FARRAY_OBJ) $(INTERSTELLAR_OBJ)
#
