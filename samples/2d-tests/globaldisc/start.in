!                           -*-f90-*-     (for Emacs)
!
!  Initialisation parameters
!
&init_pars
  cvsid='$Id: start.in,v 1.12 2005-11-15 01:06:21 wlyra Exp $',
  ip=14,                        ! debugging parameter
  xyz0  = -2.6,-2.6,-2.6,       ! first corner of box
  xyz1  =  2.6, 2.6, 2.6,       ! second corner of box
  lperi =  F  , F  , T,         ! periodic direction?

  bcx = 'a','s','s','a2'
  bcy = 's','a','s','a2'

  r_int=0.4,                   ! radius of interior spherical boundary
  r_ext=2.5,                   ! radius of exterior spherical boundary
/
&eos_init_pars
  cs0=0.05 
  rho0 = 1. 
/
&hydro_init_pars
  inituu='keplerian'
/
&density_init_pars
  initlnrho='zero' 
  lnrho_const=0.  
  plaw=0. 
  ldensity_nolog=T
/
&grav_init_pars
  ipotential='smoothed-newton', 
  g0=1.,r0_pot=0.4,n_pot=2 
/
