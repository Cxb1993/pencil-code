!  -*-f90-*-  (for Emacs)    vim:set filetype=fortran:  (for vim)
!
!  sample file for 1-D sound wave
!
&run_pars
  cvsid='$Id: run.in,v 1.4 2005-06-26 17:34:22 eos_merger_tony Exp $'
  nt=200000, it1=50, cdt=0.1, cdtv=0.4, isave=50, itorder=3,
  dsnap=20., dvid=.02,tmax=1.9,
  bcx = 'a','s','s','s','s'
  lpencil_check=T
/
&hydro_run_pars
/
&density_run_pars
  cdiffrho=0.,  !(mass diffusion not currently used)
/
&entropy_run_pars
  lcalc_heatcond_constchi=T,chi=3e-4
/
&viscosity_run_pars
  nu=0.002, nu_shock=3.
/
