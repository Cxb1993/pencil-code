#!/bin/sh
#  -*-Perl-*-
#======================================================================#
# Run the right perl version:
if [ -x /usr/local/bin/perl ]; then
  perl=/usr/local/bin/perl
elif [ -x /usr/bin/perl ]; then
  perl=/usr/bin/perl
else
  perl=`which perl| sed 's/.*aliased to *//'`
fi

exec $perl -x -S $0 "$@"     # -x: start from the following line
#======================================================================#
#! /Good_Path/perl -w 
# line 17

# Name:   auto-test
# Author: Tony Mee (A.J.Mee@ncl.ac.uk)
# Date:   10-Oct-2002
# CVS: $Id: cleanf95,v 1.1 2002-10-10 10:33:50 mee Exp $
# Description:
#   Read standard input (piped from Intel Fortran compiler with 2>&1 | 
#   i.e. from STDERR)
#   Strip all the subroutine / function lines and dump the rest back
#   out to STDERR

while(<STDIN>)
{
  next if /^\s*module subroutine/;
  next if /^\s*module function/;
  
  print STDERR $_;    
}
