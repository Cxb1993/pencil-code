!                           -*-f90-*-     (for Emacs)
!
!  Initialisation parameters
!
&init_pars
  cvsid='$Id: start.in,v 1.19 2006-08-27 20:41:30 wlyra Exp $',
  ip=14,                        ! debugging parameter
  xyz0  = -2.6,-2.6,-0.26,       ! first corner of box
  xyz1  =  2.6, 2.6, 0.26,       ! second corner of box
  lperi = T  , T  , T,         ! periodic direction?
  !lshift_origin =  F,  F,  F,

  !bcx = 's','s','a','s'!,'s','s','a','s'
  !      velgas, densgas, veldust,densdust
  !bcy = 's','s','a','s'!,'s','s','a','s'

  r_int=0.4,                   ! radius of interior spherical boundary
  r_ext=2.5,                   ! radius of exterior spherical boundary
  lcylindrical=T
/
&eos_init_pars
  cs0=0.05 
  rho0 = 1.
/
&hydro_init_pars
  inituu='keplerian'
/
&density_init_pars
  initlnrho='zero' 
  lnrho_const=0.  
  ldensity_nolog=T
/
&grav_init_pars
  ipotential='smoothed-newton', 
  g0=1.,r0_pot=0.1,n_pot=2
/
&particles_init_pars
  initxxp='random', initvvp='random'
  delta_vp0=0.001
  eps_dtog=1.0
  lparticlemesh_cic=T
/
&particles_nbody_init_pars
  initxxsp='fixed-cm'
  initvvsp='fixed-cm'
/
&planet_init_pars
  llocal_iso=T
  lramp=F
  gc=1e-4
/

