!                           -*-f90-*-     (for Emacs)
!
!  Initialisation parameters
!
&init_pars
  cvsid='$Id: start.in,v 1.2 2005-06-26 17:34:28 eos_merger_tony Exp $',
  ip=14,                        ! debugging parameter
  xyz0  = -20.,-20.,-20.,       ! first corner of box
  xyz1  =  20., 20., 20.,       ! second corner of box
  lperi =  F  , F  , T,         ! periodic direction?
  !lshift_origin =  F,  F,  F,
  bcx = 'a','s','s','a2'
  bcy = 's','a','s','a2'
  !bcz = 's','s','a','s:a2','s:a2'
  !random_gen='nr_f90',
  r_int=2.,                    ! radius of interior spherical boundary
  r_ext=18.,                    ! radius of exterior spherical boundary
  lcylindrical=T
/
&eos_init_pars
  cs0=0.7
  !rho0=1.,
/
&hydro_init_pars
  !inituu='zero'
  inituu='keplerian'
  !urand=0.05,
  !Omega=0.3906    !sqrt(g0/r**3)  !in the border
  !Omega = 2.51588  !for jupiter 5.2 AU
  Omega=.39
  lcentrifugal_force=T
/
&density_init_pars
  !initlnrho='geo-kws'
  !mpoly=1.
/
&grav_init_pars
  ipotential='smoothed-newton', g0=890., r0_pot=2.,n_pot=10
/
