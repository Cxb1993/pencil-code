start.o:     start.f90 $(start_deps)
run.o:       run.f90 $(run_deps)
test.o:      test.f90 $(test_deps)
##
## Explict dependencies for the Fourier routines to allow them to pull in
## different library routines.
##
fft.o : fft.f
#
fftpack.o : fftpack.f
#
nofourier.o: nofourier.f90 fourier.h $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o
#
fourier_fft.o: fourier_fft.f90 fft.o fourier.h $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o
#
fourier_fftpack.o: fourier_fftpack.f90 fftpack.o fourier.h $(GENERAL_OBJ) $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o
