## ---------------------------------------------------------------------- #
## Non-F90 / undetectable dependencies
## ---------------------------------------------------------------------- #
#
nodebug$(PC_META_TYPE).o:            nodebug.f90 $(CDATA_OBJ) $(MPICOMM_FIX)$(PC_META_TYPE).o
$(DEBUG_OBJ):                        debug_c.c Makefile.src Makefile.local
#$(TIMEAVG)$(PC_META_TYPE).o:        ctimeavg.local
#axeltest $(INITCOND_SPEC)$(PC_META_TYPE).o:$(INITCOND_SPEC).f90
nogsl.o: nogsl.c
	$(CC) $(CFLAGS) -c nogsl.c
gsl_wrapper.o: gsl_wrapper.c
	$(CC) $(CFLAGS) -c gsl_wrapper.c
gsl_wrapper_r8.o: gsl_wrapper_r8.c
	$(CC) $(CFLAGS) -c gsl_wrapper_r8.c 
## ---------------------------------------------------------------------- #
## Automatically generatable dependencies
## ---------------------------------------------------------------------- #
start$(PC_META_TYPE).o:     start.f90 $(start_deps)
run$(PC_META_TYPE).o:       run.f90 $(run_deps)
$(SHOCK_FINDER)$(PC_META_TYPE).o: $(SHOCK_FINDER).f90 $(shock_finder_deps)
read_videofiles$(PC_META_TYPE).o: read_videofiles.f90 $(read_videofiles_deps)
read_vectorfiles$(PC_META_TYPE).o: read_vectorfiles.f90 $(read_vectorfiles_deps)
#merge_tony var2ascii$(PC_META_TYPE).o: var2ascii.f90 $(var2ascii_deps)
#
$(CPARAM_OBJ):    $(CPARAM_SRC)  cparam$(PC_META_TYPE).local cparam$(PC_META_TYPE).inc
$(CDATA_OBJ):     $(CDATA_SRC)   $(CPARAM_OBJ)
$(GRID_OBJ):      $(GRID_SRC)    $(CDATA_OBJ) $(MPICOMM_OBJ) $(MESSAGES_OBJ)
$(GENERAL_OBJ):   $(GENERAL_SRC) $(MESSAGES_OBJ) $(CPARAM_OBJ) $(CDATA_OBJ)

#
# Explict dependencies for the Fourier routines to allow them to pull in
# different library routines
#
fft.o :      fft.f
fftpack.o :  fftpack.f
nofourier$(PC_META_TYPE).o:       nofourier.f90 fourier.h $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o
fourier_fft$(PC_META_TYPE).o:     fourier_fft.f90 fft.o fourier.h $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o
fourier_fftpack$(PC_META_TYPE).o: fourier_fftpack.f90 fftpack.o fourier.h $(MESSAGES_OBJ) $(MPICOMM_FIX).o cparam.o cdata.o

#
# Explict dependencies for the Shock routines
# different library routines
#
shock$(PC_META_TYPE).o:       shock.h   shock.f90 shock_profile.inc $(CDATA_OBJ) $(CPARAM_OBJ) $(DENSITY_OBJ) $(SUB_OBJ) boundcond$(PC_META_TYPE).o $(FARRAY_OBJ)
shock_highorder$(PC_META_TYPE).o:       shock.h   shock_highorder.f90 $(CDATA_OBJ) $(CPARAM_OBJ) $(SUB_OBJ) boundcond$(PC_META_TYPE).o $(FARRAY_OBJ)
shock_perp$(PC_META_TYPE).o:  shock.h   shock_perp.f90 shock_profile.inc $(CDATA_OBJ) $(CPARAM_OBJ) $(DENSITY_OBJ) $(SUB_OBJ) boundcond$(PC_META_TYPE).o $(FARRAY_OBJ)
noshock$(PC_META_TYPE).o:     shock.h   noshock.f90 $(CDATA_OBJ) $(CPARAM_OBJ) $(SUB_OBJ) $(FARRAY_OBJ)

